{
    "cache": false,
    "check-coverage": true,
    "extension": [".ts"],
    "include": ["src/lib/*.ts"],
    "exclude": ["coverage/**", "node_modules/**", "src/index.ts"],
    "report-dir": "./docs/coverage/",
    "temp-directory": "./.nyc_output",
    "sourceMap": true,
    "reporter": ["text", "text-summary", "cobertura", "html"],
    "all": true,
    "instrument": true,
    "branches": 80,
    "lines": 90,
    "functions": 85,
    "statements": 90
}
